convert liter to gallon
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj=new Scanner(System.in);
	    float liter=obj.nextFloat();
	    float gallon=(0.26417f * liter);
	    System.out.printf("%.2fgallon",gallon);
		
	}
}



convert inches to foot
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj=new Scanner(System.in);
	    float inches=obj.nextFloat();
	    float foot=(0.26417f * inches);
	    System.out.printf("%.2ffoot",foot);
		
	}
}


temperature in  Fahrenheit , convert it into Celsius
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj=new Scanner(System.in);
	    float far=obj.nextFloat();
	    float celsius=(far-32)*5/9f;
	    System.out.printf("%.2fcelsius",celsius);
		
	}
}


kilometer to miles
 import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner obj=new Scanner(System.in);
	    float km=obj.nextFloat();
	    float miles=km/1.6f;
	    System.out.printf("%.2fmiles",miles);
		
	}
}




divisible by 3 or not

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner abc=new Scanner(System.in);
	int a;
	a=abc.nextInt();
	int b=a%3;
	if(b==0)
	System.out.println("this number is divisible by 3");
	else
	System.out.println("it is not divisible by 3 it reminder is"+b);
	}
}


leap year

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner abc=new Scanner(System.in);
	int a;
	a=abc.nextInt();
	if(a%400==0)
	System.out.println("is leap year");
        else if(a%100==0);
        system.out.println("not leap year");
        else if(a%4==0)
        system.out.println("leapyear");
	else
	System.out.println("it is not leap year");
	}
}



day code using switch case

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner abc=new Scanner(System.in);
	int a=abc.nextInt();
	switch(a){
	case 1:
	    System.out.println("monday");
	    break;
	case 2:
	    System.out.println("tuesday");
	    break;
	case 3:
	    System.out.println("wednesday");
	    break;
	case 4:
	    System.out.println("thusday");
	    break;
	case 5:    
	    System.out.println("friday");
	    break;
	case 6:
	    System.out.println("saturday");
	    break;
	default:
	    System.out.println("enter 1-6");
	
	}  



month code using switch case


import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner abc=new Scanner(System.in);
	int a=abc.nextInt();
	switch(a){
	case 1:
	    System.out.println("january");
	    break;
	case 2:
	    System.out.println("february");
	    break;
	case 3:
	    System.out.println("march");
	    break;
	case 4:
	    System.out.println("april");
	    break;
	case 5:    
	    System.out.println("may");
	    break;
	case 6:
	    System.out.println("june");
	    break;
	case 7:
	    System.out.println("july");
	    break;
	case 8:
	    System.out.println("august");
	    break;
	case 9:
	    System.out.println("septrember");
	    break;
	case 10:
	    System.out.println("october");
	    break;
	case 11:
	    System.out.println("november");
	    break;
	case 12:
	    System.out.println("december");
	    break;
	default:
	    System.out.println("enter 1-12");
	
	}  
	
	}
}



char is upper or lower


import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner abc=new Scanner(System.in);
	char a=abc.next().charAt(0);
	Character.isUpperCase(a);
	if(Character.isUpperCase(a))
	System.out.println(a+" is upper");
	else
	System.out.println(a+" is lower");

 
	
	}
}
